<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shanzhenqiang.cloudeducationapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/protected_cloud"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignInActivity" />
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".CurriculumActivity" />
        <activity android:name=".CurriculumDetailsActivity" />
        <activity android:name=".CloudEducationHomeActivity" />
        <activity android:name=".PersonalCenterActivity" />
        <activity android:name=".MyMessageActivity" />
        <activity android:name=".MessageDetailActivity" />
        <activity android:name=".MyCurriculumActivity" />
        <activity android:name=".CurriculumMaterialActivity" />
        <activity android:name=".FooterActivity" />
        <activity android:name=".HeaderActivity" />
        <activity android:name=".OnlyTitleHeaderActivity" />
        <activity android:name=".SurfaceActivity" />
        <activity android:name=".DisplayWordActivity" />
    </application>

</manifest>